FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    ground
    {
        type            calculated;
        value           $internalField;
    }
    
    top
    {
        type            calculated;
        value           $internalField;
    }
    
    left
    {
        type            cyclic;
    }

    right
    {
        type            cyclic;
    }

    front
    {
        type            cyclic;
    }
    
    back
    {
        type            cyclic;
    }
}


// ************************************************************************* //
