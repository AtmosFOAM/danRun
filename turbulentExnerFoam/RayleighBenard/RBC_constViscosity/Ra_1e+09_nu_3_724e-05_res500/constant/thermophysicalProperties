FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          thermophysicalProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


T0        300;

thermoType
{
    type            heRhoThermo;
    energy          sensibleEnthalpy;
    specie          specie;
    equationOfState perfectGas;
    thermo          hConst;
    transport       const;
    mixture         pureMixture;
}

mixture//  dry air: 1 28.97 1005 0 1.846e-05 0.707;
{
    specie
    {
        molWeight       28.97;
    }
    thermodynamics
    {
        Cp              1005;
        Hf              0;  // reference latent heat of fusion
    }
    transport
    {
        mu              3.724e-05;
        Pr              0.707;
    }
}

pRef      100000;

