FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/9c";
projection2D XZ;

FieldsToPlot
(
    [theta filledContours 270.0 330.0 2.5 "$GMTU/colours/red_white_blue.cpt"]
    [Uf vectors 0.02 30 "black"]
);

//separateLegend true;
//region "-5000/5000/0/1.25e4";
annotate "minMax 2.5 -0.1";
boundaryMarks "1/0.5";
plotBoundaryDots false;

