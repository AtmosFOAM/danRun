FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    bottom
    {
        type            fixedValue;
        value           uniform 300.5;
    }
    top
    {
        type            fixedValue;
        value           uniform 299.5;
    }
    left
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}

