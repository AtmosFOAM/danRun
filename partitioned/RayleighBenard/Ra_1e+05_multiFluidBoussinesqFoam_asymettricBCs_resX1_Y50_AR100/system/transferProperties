FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          transferProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

cloudRadiusMax     cloudRadiusMax [0 1 0 0 0] 0.1;
cloudRadiusMin     cloudRadiusMin [0 1 0 0 0] 1e-9;
dragCoeff       10;
Ksigma Ksigma [0 2 -1 0 0] 0;
Kw Kw [0 2 -1 0 0] 1e-5;
minSigma  1e-9;
// Transfer between partitions based on horizontal divergence
wTransfer false;
divTransfer 0;
// Transfer between partitions based on laplacian(buoyancy)
buoyancyTransfer false;
bTransferDiffusivity bTransferDiffusivity  [0 2 -1 0 0] 1e6;
transferTimeScale transferTimeScale [0 1 -1 0 0] 1e-4;
transferAnomalies false;

uniformSigma    true;

